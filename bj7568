#include <stdio.h>

int main(void)
{
    int N, i, j, cnt;
    int arr[50][2]; //x축 50개(행)  y축 2개의(열) 배열

    scanf("%d", &N); //테스트 케이스 개수 입력받음

    for (i = 0; i < N; i++)
        scanf("%d %d", &arr[i][0], &arr[i][1]); 
    // 반복문을 통해 각 주어진 테스트 케이스 행마다의 몸무게와 키 입력받음

    for (i = 0; i < N; i++)
    {
        cnt = 0;

        for (j = 0; j < N; j++)
            if (arr[i][0] < arr[j][0] && arr[i][1] < arr[j][1])
                cnt++; //i의 다른 학생들과의 덩치(몸무게& 키) 비교를 통해
                       //비교 기준인 i학생보다 덩치가 큰 학생이 있다면 
                       //사후연산자를 사용하여 등수를 1씩 증가시켜줌

        printf("%d ", ++cnt); //위 i학생의 등수는 본인보다 덩치가 큰 학생의 명수 +1 이므로
                              //사전연산자를 통해 덩치가 큰 학생들의 수에 +1을 진행시켜줌.
    }
    return 0;
}
