#include <stdio.h>
int main() {
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);
	int add = a + b + c;
	
	if (add != 180)
	{
		printf("Error");
	}
	else if (a == 60 && a == b && b == c) 
	{
	    printf("Equilateral");
	}
	else if (a == b || b == c || c == a)
	{
		printf("Isosceles");
	}
	else
	{
		printf("Scalene");
	}
	return 0;
}
